<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>project1</display-name>
  
  
  <servlet>
	<servlet-name>ReimbursmentServlet</servlet-name>
	<servlet-class>com.project1.ReimbursmentServlet</servlet-class>
</servlet>

 <servlet>
	<servlet-name>LoginServlet</servlet-name>
	<servlet-class>com.project1.LoginServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>ViewTicketsServlet</servlet-name>
	<servlet-class>com.project1.ViewTicketsServlet</servlet-class>
</servlet>

<servlet>
	<servlet-name>DataServlet</servlet-name>
	<servlet-class>com.project1.DataServlet</servlet-class>
</servlet>



<servlet-mapping>
	<servlet-name>ReimbursmentServlet</servlet-name>
	<url-pattern>/ReimbursmentServlet</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>LoginServlet</servlet-name>
	<url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>ViewTicketsServlet</servlet-name>
	<url-pattern>/ViewTicketsServlet</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>DataServlet</servlet-name>
	<url-pattern>/DataServlet</url-pattern>
</servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>

